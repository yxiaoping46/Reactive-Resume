# Environment
NODE_ENV=development

# Ports
PORT=3000

# URLs
PUBLIC_URL=http://localhost:3000

# Chrome Browser (for printing)
CHROME_PORT=8080
CHROME_TOKEN=chrome_token
CHROME_URL=ws://localhost:8080

# Storage
STORAGE_ENDPOINT=localhost
STORAGE_PORT=9000
STORAGE_REGION=us-east-1
STORAGE_BUCKET=default
STORAGE_ACCESS_KEY=minioadmin
STORAGE_SECRET_KEY=minioadmin
STORAGE_USE_SSL=false
STORAGE_SKIP_BUCKET_CHECK=false

# Feature Flags (Optional)
# DISABLE_SIGNUPS=false

# Authentication Secrets
# generated with `openssl rand -base64 64`
ACCESS_TOKEN_SECRET=access_token_secret
REFRESH_TOKEN_SECRET=refresh_token_secret

# Mail Server (for e-mails)
# For testing, you can use https://ethereal.email/create
MAIL_FROM=noreply@localhost
# SMTP_URL=smtp://username:password@smtp.ethereal.email:587

# Nx Cloud (Optional)
# NX_CLOUD_ACCESS_TOKEN=

# Crowdin (Optional)
# CROWDIN_PROJECT_ID=
# CROWDIN_PERSONAL_TOKEN=

# Supabase Configuration
# Get these values from your Supabase project settings (https://app.supabase.com/project/_/settings/api)
SUPABASE_URL=your_supabase_project_url                # Project URL
SUPABASE_ANON_KEY=your_supabase_anon_key             # Project API Keys -> anon/public
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role  # Project API Keys -> service_role (Keep this secret!)